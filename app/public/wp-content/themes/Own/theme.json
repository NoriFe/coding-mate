{
    "$schema": "https://schemas.wp.org/wp/5.9/theme.json",
    "version": 2,
    "settings" : {
      "appearanceTools": true,
       "color" : {
        "defaultPalette" : false,
        "palette": [
            { "slug": "o-white", "color": "#fff", "name": "Own White" },
            { "slug": "o-gray-100", "color": "rgb(243 244 246)", "name": "Own Gray 100" },
            { "slug": "o-gray-200", "color": "rgb(229 231 235)", "name": "Own Gray 200" },
            { "slug": "o-gray-300", "color": "rgb(209 213 219)", "name": "Own Gray 300" },
            { "slug": "o-gray-400", "color": "rgb(156 163 175)", "name": "Own Gray 400" },
            { "slug": "o-gray-500", "color": "rgb(107 114 128)", "name": "Own Gray 500" },
            { "slug": "o-gray-600", "color": "rgb(75 85 99)", "name": "Own Gray 600" },
            { "slug": "o-gray-700", "color": "rgb(55 65 81)", "name": "Own Gray 700" },
            { "slug": "o-gray-800", "color": "rgb(31 41 55)", "name": "Own Gray 800" },
            { "slug": "o-gray-900", "color": "rgb(17 24 39)", "name": "Own Gray 900" },
            { "slug": "o-primary", "color": "rgb(239 68 68)", "name": "Own Primary" },
            { "slug": "o-accent", "color": "rgb(67 56 202)", "name": "Own Accent" },
            { "slug": "o-success", "color": "rgb(52 211 153)", "name": "Own Success" },
            { "slug": "o-info", "color": "rgb(96 165 250)", "name": "Own Info" },
            { "slug": "o-warning", "color": "rgb(251 146 60)", "name": "Own Warning" },
            { "slug": "o-danger", "color": "rgb(236 72 153)", "name": "Own Danger" }
                        
        ],
        "background": true,
        "text": true,
        "link": true,
        "custom": true,
        "duotone": [
            { "slug": "o-pink-sunset", "colors": ["#11245e", "#dc4379"], "name": "Own Pink Sunset"}
        ],
        "defaultGradients": false,
        "gradients": [
            { "slug": "o-summer-dog",
              "gradient": "linear-gradient(#a8ff78, #78ffd6)",
              "name": "Own Summer Dog"
            }
        ]
       },
       "blocks": {
        "core/site-title":{
            "color" : {
                "defaultPalette" : true,
                "palette": [
                    { "slug": "o-primary", "color": "#ef4444", "name": "Own Primary" }
                ],
                "link": true,
                "custom": false,
                "gradients": [],
                "customGradient": false
        
               },
               "typography": {
                "fontFamilies": [
                    {"fontFamily": "Pacifico, sams-serif",
                     "slug": "o-Pacifico",
                     "name": "Own Pacifico"    }
                ]
               
               }
        },
        "core/cover": {
            "color": {
                "duotone": [],
                "customDuotone": false
            }
        },
        "core/preformatted": {
            "typography": {
                "fontSizes": [],
                "customFontSize": false
            }
        }
       },
       "border": {
        "color": true,
        "radius": true,
        "width": true,
        "style": true
       },
       "typography": {
        "fontFamilies": [
            {"fontFamily": "Rubik, sams-serif",
             "slug": "o-rubik",
             "name": "Own Rubik"    }
        ],
        "fontSizes": [
            {"slug": "small", "size": "0.75rem", "name": "Small"},
            {"slug": "medium", "size": "1.25rem", "name": "Medium"},
            {"slug": "large", "size": "2.25rem", "name": "Large"},
            {"slug": "extra-large", "size": "3rem", "name": "Extra Large"},
            {"slug": "gigantic", "size": "3.75rem", "name": "Gigantic"}
        ],
        "lineHeight": true,
        "dropCap": true,
        "fontWeight": true,
        "fontStyle": true,
        "textTransform": true,
        "letterSpacing": true,
        "textDecoration": true
       },
       "layout": {
        "contentSize": "800px",
        "wideSize": "1100px"
       },
       "spacing": {
         "margin": true,
         "padding": true,
         "units": [
            "px",            
            "rem",
            "vh",
            "vw",
            "%"
         ],
         "blockGap": true
       }
            
    },
    "styles":{
        "color":{
            "text": "var(--wp--preset--color--o-gray-700)",
            "background": "var(--wp--preset--color--o-white)"
        },
        "elements": {
            "link":{
                "color":{
                    "text": "var(--wp--preset--color--o-primary)"
                }
            }
        },
        "blocks": {
            "core/button": {
                "color":{
                    "gradient": "var(--wp--preset--color--o-summer-dog)"
                }
            },
            "core/pullquote": {
                "border": {
                    "width": "2px",
                    "style": "solid",
                    "radius": "10px",
                    "color": "var(--wp--preset--color--o-primary)"

                }
            },
            "core/site-title": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--o-pacifico)"
                }
            }
        },
        "typography": {
           "fontFamily": "var(--wp--preset--font-family--o-rubik)",
           "fontSize": "16px",
           "fontStyle": "normal",
           "fontWeight": "normal",
           "lineHeight": "inherit",
           "textDecoration": "none",
           "textTransform": "none"
        },
        "spacing": {
            "margin": {
                "top": "0px",
                "right": "0px",
                "bottom": "0px",
                "left": "0px"
            },
            "padding": {
                "top": "0px",
                "right": "0px",
                "bottom": "0px",
                "left": "0px"
            },
            "blockGap": "4rem"
        }
    }
}